generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  uid        String @id
  first_name String
  last_name  String
  user_type  String
}

model login_credentials {
  uid      String @id
  email    String @unique
  password String
}
